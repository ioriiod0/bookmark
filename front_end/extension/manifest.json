{
  "name": "My Bookmarks",
  "version": "1.0",
  "description": "bookmarks manager",
  "permissions": [
    "bookmarks",
    "http://*/"
  ],
  "background": {
    "page": "back_ground.html"
  },
  "browser_action": {
      "default_title": "My Bookmarks.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
